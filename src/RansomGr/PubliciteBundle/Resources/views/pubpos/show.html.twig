{% extends 'base.html.twig' %}

{% block body %}
    <h1>Pubpo</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pubPo.id }}</td>
            </tr>
            <tr>
                <th>Pos</th>
                <td>{{ pubPo.pos }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ pubPo.price }}</td>
            </tr>
            <tr>
                <th>Clickprice</th>
                <td>{{ pubPo.Clickprice }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('Admin_Publicite_Position_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('Admin_Publicite_Position_edit', { 'id': pubPo.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
